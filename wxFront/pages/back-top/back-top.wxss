page {
  background: #fff;
}
page .skeleton-item {
  padding-bottom: 48rpx;
}
page .container-flex {
  display: flex;
  justify-content: space-between;
  margin-top: 48rpx;
  flex-wrap: wrap;
}
.desc {
  color: rgba(19, 7, 7, 0.4);
  margin-top: 32rpx;
  font-size: 36rpx;
  font-weight: 400;
  line-height: 44rpx;
  margin-bottom: 48rpx;
  padding-left: 16rpx;
  padding-right: 16rpx;
}
.childBox {
  transition: height 0.3s;
}
.childBox .child {
  box-sizing: border-box;
  border-bottom: 1rpx solid #e5e5e5;
  height: 112rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 32rpx;
  margin-right: 32rpx;
  font-size: 32rpx;
  opacity: 0.9;
}
.video-image {
  width: 92%;
  border-radius: 10rpx;
  margin-top: 15rpx;
  margin-left: 30rpx;
}