<view class="main">
  <view class="title-wrap">
    <image style="width: 453rpx; height: 229rpx; display: block; box-sizing: border-box" class="title-icon" mode="aspectFit" src="/assets/gaoshu.png" aria-label="TDesign Logo" />
  </view>
  <view class="desc"> 高中数学在线练习课程 </view>
  <pull-down-list
    wx:for="{{list}}"
    name="{{item.name}}"
    icon="{{item.icon}}"
    wx:key="name"
    childArr="{{item.childArr}}"
    bind:click="clickHandle"
  />
</view>
